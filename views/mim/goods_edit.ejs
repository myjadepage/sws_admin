                                        alert("추가구성명을 입력해 주세요.");
                                        $adtName.focus();
                                        invalid = true;
                                        return false;
                                    }

                                    $('input[name="adtItemIdx' + adtNo + '"]').each(function() {
                                        var $tr = $(this).closest('tr'),
                                            $adtItem = $('input[name="adtItem' + adtNo + '"]', $tr),
